{
    "name": "alai/login-bundle",
    "description": "A Symfony bundle to integrate OpenId login",
    "type": "symfony-bundle",
    "license": "proprietary",
    "autoload": {
        "psr-4": {
            "Facile\\OpenIdBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "lcobucci/jwt": "^3.2",
        "symfony/config": "^3.4",
        "symfony/dependency-injection": "^3.4",
        "symfony/http-kernel": "^3.4",
        "symfony/routing": "^3.4",
        "symfony/security-core": "^3.4"
    },
    "require-dev": {
        "drew/debug-statements-fixers": "^0.5",
        "facile-it/facile-coding-standard": "^0.3",
        "jangregor/phpstan-prophecy": "^0.3",
        "phpstan/phpstan": "^0.11.3",
        "phpstan/phpstan-phpunit": "^0.11",
        "phpunit/phpunit": "^8.0.5",
        "symfony/phpunit-bridge": "^4.2"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs-check": "php-cs-fixer fix --dry-run --verbose",
        "cs-fix": "php-cs-fixer fix --verbose"
    }
}
